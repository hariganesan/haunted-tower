package  {		import flash.display.MovieClip;			public class Defender extends MovieClip {		private var Parent:MovieClip;		private var strength:int;		private var projectileSpeed:int;				public function Defender(p:MovieClip, s:int, v:int, X:int, Y:int) {			Parent = p;			strength = s;			projectileSpeed = v;						x = X;			y = Y;			Parent.addChild(this);		}				public function createProjectile() {			var proj:Projectile = new Projectile(Parent, strength, projectileSpeed, x, y);		}	}	}