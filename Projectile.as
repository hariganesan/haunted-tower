package  {		import flash.display.MovieClip;			public class Projectile extends MovieClip {		private var Parent:MovieClip; // Main		private var strength:int;		private var speed:int;		private var id:int; // int given from Main				public function Projectile(p:MovieClip, s:int, v:int, X:int, Y:int) {			Parent = p;			strength = s;			speed = v;						x = X;			y = Y;			Parent.addChild(this);			id = Parent.addProjectile(this);		}				public function destroy() {			Parent.removeChild(this);			Parent.removeProjectile(this);		}				// accessor functions				public function getStrength() {			return strength;		}				public function getSpeed() {			return speed;		}	}	}