package  {		import flash.display.MovieClip;	import flash.events.Event;			public class Projectile extends MovieClip {		private var Parent:MovieClip; // Main		private var strength:int;		private var speed:int;		private var id:int; // int given from Main				public function Projectile(p:MovieClip, s:int, v:int, X:int, Y:int):void {			Parent = p;			strength = s;			speed = v;						x = X;			y = Y;			Parent.addChild(this);			id = Parent.addProjectile(this);			this.addEventListener(Event.ENTER_FRAME, checkHit);		}				public function destroy():void {			Parent.removeChild(this);			Parent.removeProjectile(this); // updates count on main		}				private function checkHit(e:Event):void {			if (Parent.checkCollisions(this)) {				destroy();			// check bounds			} else if (x < 0 || x > stage.stageWidth || y < 0 || y > stage.stageHeight) {				destroy();			}		}				// accessor functions				public function getStrength():int {			return strength;		}				public function getSpeed():int {			return speed;		}	}	}